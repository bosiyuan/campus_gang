  \r\n    <!-- <pagination\r\n      v-show=\"total>0\"\r\n      :total=\"total\"\r\n      :page.sync=\"queryParams.pageNum\"\r\n      :limit.sync=\"queryParams.pageSize\"\r\n      @pagination=\"getList\"\r\n    /> -->\r\n\r\n    <!-- 添加或修改remark对话框 -->\r\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body>\r\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\r\n      \r\n        <el-form-item label=\"星级\" prop=\"star\">\r\n            <el-rate\r\n                v-model=\"form.star\"\r\n                show-text>\r\n            </el-rate>\r\n        </el-form-item>\r\n        <el-form-item label=\"评价内容\" prop=\"remark\">\r\n          <el-input v-model=\"form.remark\" placeholder=\"请输入评价内容\" />\r\n        </el-form-item>\r\n      \r\n      </el-form>\r\n\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { listRemark } from \"@/api/remark/remark\";\r\nimport {mapState} from \"vuex\"\r\n\r\nexport default {\r\n  name: \"Remark\",\r\n  data() {\r\n    return {\r\n        // 遮罩层\r\n        loading: true,\r\n        // 选中数组\r\n        ids: [],\r\n        // 非单个禁用\r\n        single: true,\r\n        // 非多个禁用\r\n        multiple: true,\r\n        // 显示搜索条件\r\n        showSearch: true,\r\n        // 总条数\r\n        total: 0,\r\n        // remark表格数据\r\n        remarkList: [],\r\n        // 弹出层标题\r\n        title: \"\",\r\n        // 是否显示弹出层\r\n        open: false,\r\n\r\n        // 查询参数\r\n        queryParams: {\r\n            pageNum: 1,\r\n            pageSize: 10,\r\n            star: null,\r\n            taskId: null,\r\n            acceptId: null,\r\n            publishId: null\r\n        },\r\n\r\n        // 表单参数\r\n        form: {},\r\n        // 表单校验\r\n        rules: {\r\n        },\r\n        tasks: [],\r\n    };\r\n  },\r\n  computed: {\r\n      ...mapState('user', ['user'])\r\n  },\r\n  created() {\r\n    this.getList();\r\n    this.retrieveData();\r\n  },\r\n  methods: {\r\n    /** 查询‘评价我的’列表 搜索接受任务人为自己 */\r\n    getList() {\r\n      this.loading = true;\r\n      listRemark(\r\n        {\"acceptId\" : this.user.id}\r\n      ).then(res => {\r\n        this.remarkList = res.data.rows;\r\n        console.log(this.remarkList);\r\n        this.total = res.total;\r\n        this.loading = false;\r\n      });\r\n    },\r\n    //任务列表\r\n    retrieveData() {\r\n        this.$get(\"/task/published\", {id: this.user.id}).then(res => {\r\n        // console.log(res.data.task)\r\n        this.tasks = res.data.task\r\n        })\r\n    },\r\n    // 取消按钮\r\n    cancel() {\r\n      this.open = false;\r\n      this.reset();\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        id: null,\r\n        star: null,\r\n        remark: null,\r\n        taskId: null,\r\n        acceptId: null,\r\n        publishId: null\r\n      };\r\n    //   this.resetForm(\"form\");\r\n    },\r\n    /** 搜索按钮操作 */\r\n    handleQuery() {\r\n      this.queryParams.pageNum = 1;\r\n      this.getList();\r\n    },\r\n    /** 重置按钮操作 */\r\n    resetQuery() {\r\n      this.resetForm(\"queryForm\");\r\n      this.handleQuery();\r\n    },\r\n    // 多选框选中数据\r\n    handleSelectionChange(selection) {\r\n      this.ids = selection.map(item => item.id)\r\n      this.single = selection.length!==1\r\n      this.multiple = !selection.length\r\n    },\r\n  \r\n    \r\n  }\r\n};\r\n</script>\r\n"],"mappings":"AAyEA,SAAAA,UAAA;AACA,SAAAC,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACA;MACAC,OAAA;MACA;MACAC,GAAA;MACA;MACAC,MAAA;MACA;MACAC,QAAA;MACA;MACAC,UAAA;MACA;MACAC,KAAA;MACA;MACAC,UAAA;MACA;MACAC,KAAA;MACA;MACAC,IAAA;MAEA;MACAC,WAAA;QACAC,OAAA;QACAC,QAAA;QACAC,IAAA;QACAC,MAAA;QACAC,QAAA;QACAC,SAAA;MACA;MAEA;MACAC,IAAA;MACA;MACAC,KAAA,GACA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,GAAAtB,QAAA;EACA;EACAuB,QAAA;IACA,KAAAC,OAAA;IACA,KAAAC,YAAA;EACA;EACAC,OAAA;IACA;IACAF,QAAA;MACA,KAAArB,OAAA;MACAJ,UAAA,CACA;QAAA,iBAAA4B,IAAA,CAAAC;MAAA,CACA,EAAAC,IAAA,CAAAC,GAAA;QACA,KAAArB,UAAA,GAAAq