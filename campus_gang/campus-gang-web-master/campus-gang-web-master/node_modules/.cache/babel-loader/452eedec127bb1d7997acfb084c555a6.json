            }\r\n        },\r\n        data() {\r\n            return {\r\n                //学校\r\n                schools:[],\r\n                tasks: [],\r\n                ruleForm: {\r\n                    id: 0,\r\n                    state: true\r\n                },\r\n                options: [\r\n                    {label: \"正常\", value: \"0\"},\r\n                    {label: \"禁用\", value: \"1\"}\r\n\r\n                ],\r\n                input:\"\",\r\n                select: 'entire'\r\n            }\r\n        },\r\n        created() {\r\n            this.newList();\r\n            this.$get(\"school\")\r\n            .then(res => {\r\n                this.schools = res.data.schools\r\n            })\r\n        },\r\n\r\n        filters: {\r\n            formatDate(time) {\r\n                let date = new Date(time);\r\n                return formatDate(date, 'yyyy-MM-dd hh:mm');\r\n            }\r\n        }\r\n\r\n    }\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n    .content {\r\n        padding: 0 1%;\r\n\r\n    }\r\n\r\n    .center {\r\n        width: 80%;\r\n        margin-bottom: 30px;\r\n    }\r\n\r\n    /deep/ .el-select .el-input {\r\n        width: 200px;\r\n    }\r\n\r\n    /deep/ .input-with-select .el-input-group__prepend {\r\n        background-color: #fff;\r\n    }\r\n\r\n\r\n\r\n    .form {\r\n        margin: 0 22px;\r\n    }\r\n</style>"],"mappings":"AA+FA,SAAAA,UAAA;AACA;EACAC,IAAA;EACAC,OAAA;IACAC,QAAA;MACA,SAAAC,MAAA;QACA,KAAAC,OAAA;MACA;QACA,KAAAC,KAAA;UAAA,WAAAF;QAAA,GACAG,IAAA,CAAAC,GAAA;UACA,KAAAC,KAAA,GAAAD,GAAA,CAAAE,IAAA,CAAAD,KAAA;QACA;MACA;IACA;IACAE,YAAA;MACA,SAAAC,KAAA,YAEA;IACA;IAEAC,IAAAC,EAAA,EAAAC,KAAA;MACA,IAAAA,KAAA;QACA,KAAAC,IAAA,YAAAF,EAAA,EACAP,IAAA,CAAAC,GAAA;UACA,KAAAS,UAAA,OAAAT,GAAA,CAAAE,IAAA,CAAAQ,GAAA;UACA,KAAAb,OAAA;QACA;MACA;QACA,KAAAc,IAAA;MACA;IACA;IAEAd,QAAA;MACA,KAAAe,IAAA,UACAb,IAAA,CAAAc,EAAA;QACA,KAAAZ,KAAA,GAAAY,EAAA,CAAAX,IAAA,CAAAY,IAAA;QACA;MACA;IACA;IACAC,UAAAC,IAAA;MACA,IAAAC,IA