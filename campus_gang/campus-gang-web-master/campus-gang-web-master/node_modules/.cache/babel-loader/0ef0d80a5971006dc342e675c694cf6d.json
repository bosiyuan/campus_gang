 [];\r\n                        for (let j = 0;j<item['depts'][i]['classes'].length;j++){\r\n                            children.push(\r\n                                {\r\n                                    classesId : item['depts'][i]['classes'][j].id,\r\n                                    classesname:item['depts'][i]['classes'][j].name,\r\n                                    uuid : this.guid2()\r\n                                }\r\n                            )\r\n                        }\r\n                        depts[i].children = children\r\n                    }\r\n                    return{\r\n                            schoolId : item.id,\r\n                            name : item.name,\r\n                            children : depts,\r\n                            uuid : this.guid2()\r\n                    }\r\n                })\r\n            }\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n    .content {\r\n        padding: 0 1%;\r\n    }\r\n</style>"],"mappings":";AAiFA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;MAEAC,OAAA;MACA;MACAC,KAAA;MACA;MACAC,IAAA;MACA;MACAC,IAAA;MACA;MACAC,KAAA;IACA;EACA;EACAC,OAAA;IACAC,MAAA;MACA,SAAAC,GAAA;QACA,aAAAC,IAAA,CAAAC,MAAA,mBAAAC,QAAA,KAAAC,SAAA;MACA;MACA,OAAAJ,EAAA,KAAAA,EAAA,WAAAA,EAAA,WAAAA,EAAA,WAAAA,EAAA,WAAAA,EAAA,KAAAA,EAAA,KAAAA,EAAA;IACA;IAEA;;IAEA;IACAK,UAAAC,CAAA,EAAAC,CAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,CAAA,CAAAG,QAAA,CAAAC,MAAA;MACA;MACA,IAAAJ,CAAA,CAAAK,QAAA;QACA,KAAApB,KAAA,GAAAe,CAAA;QACA,KAAAM,KAAA;QACA,KAAAnB,KAAA;QACA,KAAAC,IAAA;MACA;IACA;IAEA;IACAmB,OAAA;MACAN,OAAA,CAAAC,GAAA,MAAAjB,KAAA,CAAAkB,QAAA;MACA,KAAAK,KAAA,SAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,SAAArB,IAAA,CAAAsB,EAAA;YACAC,YAAA,MAAAvB,IAAA,EAAAwB,IAAA,CAAAC,QAAA;cACA,KAAAC,QAAA;cACA,KAAA3B,IAAA;cACA,KAAA4B,OAAA;YACA;UACA;YACA,KAAAC,KAAA;cACA,aAAA5B,IAAA,CAAAN;YACA,GAAA8B,IAAA,CAAAC,QAAA;cACA,KAAAC,QAAA;cACA,KAAAC,OAAA;cACA,KAAAC,KAAA;gBACA,iBAAAhC,KAAA,CAAAoB,QAAA;gBACA,aAAAhB,IAAA,CAAA6B;cACA,GAAAL,I