me = this.form.deptname;\r\n                                for(let i=0 ;i<this.value.children.length;i++){\r\n                                    if(this.value.children[i].deptname==name){\r\n                                        this.$message(\"新增专业成功\");\r\n                                        this.newList();\r\n                                        this.$post(\"/class\",{\r\n                                        \"schoolId\":this.value.schoolId,\r\n                                        \"deptId\":this.value.children[i].deptsId,\r\n                                        \"name\": this.form.classesname,\r\n                                        }).then(response => {\r\n                                            this.$message(\"新增班级成功\");\r\n                                            this.newList();\r\n                                        });\r\n                                    }\r\n                                }\r\n                                \r\n                            });\r\n                            this.$message(\"新增成功\");\r\n                            this.open = false;\r\n                            this.newList();\r\n                            });\r\n                    }\r\n                }\r\n            });\r\n            },\r\n\r\n\r\n\r\n            // 取消按钮\r\n            cancel() {\r\n            this.open = false;\r\n            },\r\n\r\n            handleDelete(a,b){\r\n                console.log(b);                \r\n                if(b.schoolId != null){\r\n                    this.$del(\"/school/\"+b.schoolId)\r\n                    .then((res) => {\r\n                        this.$notifyMsg(\"成功\", res.data.msg, \"success\")\r\n                        this.newList()\r\n                    })\r\n                }else if(b.deptsId != null){\r\n                     this.$del(\"/dept/\"+b.deptsId)\r\n                        .then((res) => {\r\n                            this.$notifyMsg(\"成功\", res.data.msg, 