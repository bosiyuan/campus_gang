AACvB;AACAC,EAAAA,OAAO,EAAE,uBAFc;AAGvBa,EAAAA,OAAO,EAAC;AACJ,oBAAe;AADX,GAHe;AAMvB;AACAZ,EAAAA,OAAO,EAAE;AAPc,CAAb,CAAd,C,CAUA;;AACAW,OAAO,CAACV,YAAR,CAAqBS,QAArB,CAA8BP,GAA9B,CAAkC,UAAUO,QAAV,EAAoB;AAClDhB,EAAAA,SAAS,CAACa,IAAV,GADkD,CACjC;AACjB;;AACA,SAAOG,QAAP;AACH,CAJD,EAIG,UAAUJ,KAAV,EAAiB;AAChBZ,EAAAA,SAAS,CAACa,IAAV,GADgB,CACC;AACjB;;AACA,SAAOC,OAAO,CAACC,MAAR,CAAeH,KAAf,CAAP;AACH,CARD,E,CAUA;;AACAK,OAAO,CAACV,YAAR,CAAqBC,OAArB,CAA6BC,GAA7B,CAAiC,UAAUC,MAAV,EAAkB;AAC/CV,EAAAA,SAAS,CAACW,KAAV,GAD+C,CAC7B;AAClB;;AACA,SAAOD,MAAP;AACH,CAJD,EAIG,UAAUE,KAAV,EAAiB;AAChBZ,EAAAA,SAAS,CAACa,IAAV,GADgB,CACC;AACjB;;AACA,SAAOC,OAAO,CAACC,MAAR,CAAeH,KAAf,CAAP;AACH,CARD;;AAYA,IAAIO,GAAG,GAAG,SAANA,GAAM,CAAUC,GAAV,EAAeC,MAAf,EAAuB;AAC7B,SAAOlB,QAAQ,CAACgB,GAAT,CAAaC,GAAb,EAAkB;AAACC,IAAAA,MAAM,EAANA;AAAD,GAAlB,CAAP;AACH,CAFD;;AAIA,IAAIC,IAAI,GAAG,SAAPA,IAAO,CAAUF,GAAV,EAAeC,MAAf,EAAuB;AAC9B;AACA,SAAOlB,QAAQ,CAACmB,IAAT,CAAcF,GAAd,EAAmBnB,EAAE,CAACsB,SAAH,CAAaF,MAAb,CAAnB,CAAP;AACH,CAHD,C,CAIA;AACA;AACA;;;AAEA,IAAIG,GAAG,GAAG,SAANA,GAAM,CAAUJ,GAAV,EAAeC,MAAf,EAAuB;AAC7B,SAAOlB,QAAQ,CAACqB,GAAT,CAAaJ,GAAb,EAAkBK,IAAI,CAACF,SAAL,CAAeF,MAAf,CAAlB,CAAP;AACH,CAFD;;AAIA,IAAIK,GAAG,GAAG,SAANA,GAAM,CAAUN,GAAV,EAAeC,MAAf,EAAuB;AAC7B,SAAOlB,QAAQ,CAACwB,MAAT,CAAgBP,GAAhB,EAAqB;AAACC,IAAAA,MAAM,EAANA;AAAD,GAArB,CAAP;AACH,CAFD,C,CAIA;;;AACA,SACIF,GADJ,EACSK,GADT,EACcE,GADd,EACmBJ,IADnB","sourcesContent":["// 工具包\nimport axios from \"axios\"\nimport NProgress from 'nprogress'\nimport 'nprogress/nprogress.css'\n\nconst Qs = require(\"querystring\");\n\n/**\n * instance封装axios\n */\nvar instance = axios.create({\n    // 本地測試\n    baseURL: 'http://localhost:8080',\n    // headers:{\n    //     'Content-Type':'application/json;charset=UTF-8'\n    // },\n    // 超时时间\n    timeout: 20000\n});\n\n// 添加请求拦截器\ninstance.interceptors.request.use(function (config) {\n    NProgress.start();//开启\n    // 在发送请求之前做些什么\n    return config;\n}, function (error) {\n    NProgress.done();//关闭\n    // 对请求错误做些什么\n    return Promise.reject(error);\n});\n\n// 添加响应拦截器\ninstance.interceptors.response.use(function (response) {\n    NProgress.done();//关闭\n    // 对响应数据做点什么\n    return response;\n}, function (error) {\n    NProgress.done();//关闭\n    // 对响应错误做点什么\n    return Promise.reject(error);\n});\n\n/**\n * service封装axios\n */\nvar service = axios.create({\n    // 本地測試\n    baseURL: 'http://localhost:8080',\n    headers:{\n        'Content-Type':'application/json;charset=UTF-8'\n    },\n    // 超时时间\n    timeout: 20000\n});\n\n// 添加响应拦截器\nservice.interceptors.response.use(function (response) {\n    NProgress.done();//关闭\n    // 对响应数据做点什么\n    return response;\n}, function (error) {\n    NProgress.done();//关闭\n    // 对响应错误做点什么\n    return Promise.reject(error);\n});\n\n// 添加请求拦截器\nservice.interceptors.request.use(function (config) {\n    NProgress.start();//开启\n    // 在发送请求之前做些什么\n    return config;\n}, function (error) {\n    NProgress.done();//关闭\n    // 对请求错误做些什么\n    return Promise.reject(error);\n});\n\n\n\nlet get = function (url, params) {\n    return instance.get(url, {params});\n}\n\nlet post = function (url, params) {\n    //form\n    return instance.post(url, Qs.stringify(params));\n}\n// let posts = function (url, params) {\n//     return service.post(url, JSON.stringify(params));\n// }\n\nlet put = function (url, params) {\n    return instance.put(url, JSON.stringify(params));\n}\n\nlet del = function (url, params) {\n    return instance.delete(url, {params});\n}\n\n// 导出get和post方法\nexport {\n    get, put, del, post\n}"]}]}