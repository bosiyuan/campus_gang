A,CAAA,IAAA,CAAA;AAAA,aAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,cAAA,IAAA;AACA,cAAA,QAAA,CAAA,oBAAA,GAAA,KAAA;;AACA,cAAA,KAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA,EAAA,SAAA;;AACA,cAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,gBAAA;AACA,aANA;AAOA,WAVA,MAUA;AACA,YAAA,IAAA;AACA;AACA;AApBA,OAAA,EAqBA,KArBA,CAqBA,YAAA,CACA,CAtBA;AAuBA;AAzBA,GARA;AAoCA,EAAA,QAAA,oBACA,QAAA,CAAA,MAAA,EAAA,CAAA,MAAA,CAAA,CADA,CApCA;AAuCA,EAAA,OAvCA,qBAuCA;AAAA;;AACA;AACA,SAAA,IAAA,CAAA,OAAA,EAAA;AAAA,MAAA,EAAA,EAAA,KAAA,IAAA,CAAA;AAAA,KAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,KAHA;AAKA,GA9CA;AAgDA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,IADA,EACA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,CAAA,IAAA,CAAA;AACA,aAAA,WAAA,CAAA,IAAA,EAAA,kBAAA,CAAA;AACA;AAJA;AAhDA,CAAA","sourcesContent":["<template>\n    <div class=\"content\">\n        <el-alert\n                :title=\"user.school.name + '- - -任务平台共' + tasks.length + '个任务'\"\n                :closable=\"false\"\n                type=\"info\">\n        </el-alert>\n\n        <div>\n            <el-card class=\"box-card\">\n                <div slot=\"header\" class=\"clearfix\">\n                    <span>任务</span>\n                    <el-button style=\"float: right; padding: 3px 0\" type=\"text\">最新</el-button>\n                </div>\n                <el-card class=\"box-card\" v-for=\"item in tasks\" v-if=\"item.state == 0\">\n                    <div slot=\"header\" class=\"clearfix\"\n                         style=\"display: flex; align-items: center; justify-content: space-between\">\n                        <span>\n                            <el-tag type=\"success\" style=\"margin-right: 5px\">待解决</el-tag>\n                            {{item.taskTitle}}\n                            <i class=\"el-icon-money\" style=\"margin-left: 20px;color: red;\"> {{item.reward}}元</i>\n                        </span>\n                        <el-button style=\"float: right; padding: 3px 0\" type=\"text\"\n                                   v-show=\"user.id != item.publish.id\" @click=\"acceptTask(item.id)\">接受任务\n                        </el-button>\n                        <el-button style=\"float: right; padding: 3px 0\" type=\"text\"\n                                   v-show=\"user.id == item.publish.id\">本人任务\n                        </el-button>\n                    </div>\n                    <div class=\"text item\">\n                        <p class=\"el-icon-s-custom\">{{item.publish.username}}<span style=\"margin-left: 10px;\">{{item.taskContext}}</span>\n                        </p>\n                        <span style=\"float: right\">{{item.createTime | formatDate}}</span>\n                    </div>\n                </el-card>\n                <div style=\"text-align: center\" v-if=\"tasks.length == 0\">\n                    <span><i class=\"el-icon-refresh-right\"></i>暂无任务</span>\n                </div>\n\n            </el-card>\n        </div>\n    </div>\n</template>\n\n<script>\n    import {mapMutations, mapState} from \"vuex\";\n    import {formatDate} from '@/util/date';\n\n    export default {\n        name: \"Index\",\n        data() {\n            return {\n                tasks: [],\n            }\n        },\n\n        methods: {\n            acceptTask(id) {\n                this.$msgbox({\n                    title: '提示',\n                    message: '确定接受此任务吗？',\n                    showCancelButton: true,\n                    confirmButtonText: '确定',\n                    cancelButtonText: '取消',\n                    beforeClose: ((action, instance, done) => {\n                        if (action == 'confirm') {\n                            // instance.confirmButtonLoading = true;\n                            instance.confirmButtonText = '执行中...';\n                            this.$put('task/takerAccept', {id: id, \"acceptId\": this.user.id})\n                                .then((res) => {\n                                    done();\n                                    instance.confirmButtonLoading = false;\n                                    this.$msg(res.data.msg, \"success\");\n                                    this.$router.push('/home/accepted');\n                                })\n                        } else {\n                            done();\n                        }\n                    })\n                }).catch(() => {\n                })\n            },\n        },\n\n        computed: {\n            ...mapState('user', ['user'])\n        },\n        created() {\n            // console.log(this.user)\n            this.$get(\"/task\", {id: this.user.id})\n                .then((res) => {\n                    this.tasks = res.data.task\n            })\n\n        },\n\n        filters: {\n            formatDate(time) {\n                let date = new Date(time);\n                return formatDate(date, 'yyyy-MM-dd hh:mm');\n            }\n        }\n    }\n</script>\n\n<style scoped lang=\"less\">\n    .content {\n        background: #FFf;\n        margin: 0 15px;\n        padding: 15px;\n\n        .card h2 {\n            font-weight: normal;\n            font-size: 18px;\n\n            span {\n                font-size: 12px;\n                display: inline-block;\n                border: 1px solid red;\n                padding: 1px 3px;\n            }\n        }\n    }\n\n\n    /deep/ .el-alert--info.is-light {\n        height: 50px;\n    }\n\n    /deep/ .el-select .el-input {\n        width: 130px;\n    }\n\n    /deep/ .input-with-select .el-input-group__prepend {\n        background-color: #fff;\n    }\n\n    /deep/ .el-card {\n        margin-bottom: 20px;\n    }\n</style>"],"sourceRoot":"src/views/PC/user/children"}]}