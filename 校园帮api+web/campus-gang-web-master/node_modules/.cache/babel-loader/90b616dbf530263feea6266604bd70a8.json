A,EAAA,QAAA,oBACA,QAAA,CAAA,MAAA,EAAA,CAAA,MAAA,CAAA,CADA,CAfA;AAkBA,EAAA,OAAA,kCACA,YAAA,CAAA,MAAA,EAAA,CAAA,SAAA,CAAA,CADA;AAEA,IAAA,OAFA,mBAEA,IAFA,EAEA,KAFA,EAEA,CAEA,CAJA;AAKA;AACA,IAAA,WANA,uBAMA,GANA,EAMA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,IAAA,GAAA,CAAA,KAAA,IAAA;AACA,KATA;AAUA;AACA,IAAA,MAXA,kBAWA,EAXA,EAWA;AAAA;;AACA,WAAA,OAAA,CAAA,KAAA,CAAA;AACA,QAAA,KAAA,EAAA,MADA;AAEA,QAAA,OAAA,EAAA,SAFA;AAGA,QAAA,gBAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,WAAA,EAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,OAAA,EAAA;AAAA,YAAA,EAAA,EAAA,KAAA,CAAA,IAAA,CAAA;AAAA,WAAA,EACA,IADA,CACA,UAAA,EAAA,EAAA;AACA,YAAA,cAAA,CAAA,OAAA,CAAA,MAAA,EAAA,IAAA,CAAA,SAAA,CAAA,EAAA,CAAA,IAAA,CAAA,IAAA,CAAA;;AACA,YAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,KAAA,CAAA,cAAA,CAAA,OAAA,CAAA,MAAA,CAAA,CAAA;AACA,WAJA;;AAKA,UAAA,KAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA,EAAA,SAAA;;AACA,UAAA,KAAA,CAAA,OAAA;AACA,SATA;AAUA,OAfA,EAeA,KAfA,CAeA,YAAA,CAEA,CAjBA;AAkBA,KA9BA;AA+BA;AACA,IAAA,OAhCA,qBAgCA;AAAA;;AACA,WAAA,IAAA,CAAA,iBAAA,EAAA;AAAA,QAAA,EAAA,EAAA,KAAA,IAAA,CAAA;AAAA,OAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAHA;AAIA,KArCA;AAsCA;AACA,IAAA,UAvCA,wBAuCA;AAAA;;AACA,WAAA,IAAA,CAAA,gBAAA,EAAA;AAAA,QAAA,EAAA,EAAA,KAAA,IAAA,CAAA;AAAA,OAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CADA,CAEA;AACA,OAJA;AAKA,KA7CA;AA8CA;AACA,IAAA,QA/CA,oBA+CA,EA/CA,EA+CA;AAAA;;AACA,WAAA,OAAA,CAAA,KAAA,CAAA;AACA,QAAA,KAAA,EAAA,IADA;AAEA,QAAA,OAAA,EAAA,cAFA;AAGA,QAAA,gBAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,UAAA,EAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA,EAAA,SAAA;;AACA,UAAA,MAAA,CAAA,OAAA;AACA,SAJA;AAKA,OAVA,EAUA,KAVA,CAUA,YAAA,CAEA,CAZA;AAaA,KA7DA;AA8DA;AACA,IAAA,UA/DA,sBA+DA,EA/DA,EA+DA;AAAA;;AACA,WAAA,OAAA,CAAA,KAAA,CAAA;AACA,QAAA,KAAA,EAAA,IADA;AAEA,QAAA,OAAA,EAAA,UAFA;AAGA,QAAA,gBAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,uBAAA,EAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA,EAAA,SAAA;;AACA,UAAA,MAAA,CAAA,UAAA;AACA,SAJA;AAKA,OAVA,EAUA,KAVA,CAUA,YAAA,CAEA,CAZA;AAaA;AA7EA,IAlBA;AAiGA,EAAA,OAjGA,qBAiGA;AACA,QAAA,KAAA,MAAA,CAAA,MAAA,CAAA,KAAA,EAAA;AACA,WAAA,MAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,KAAA;AACA;;AACA,SAAA,OAAA;AACA,SAAA,UAAA;AACA,GAvGA;AAwGA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,IADA,EACA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,CAAA,IAAA,CAAA;AACA,aAAA,WAAA,CAAA,IAAA,EAAA,kBAAA,CAAA;AACA;AAJA;AAxGA,CAAA","sourcesContent":["<template>\n    <div class=\"task\">\n        <van-tabs @click=\"onClick\" v-model=\"active\">\n            <van-tab title=\"我发布的\">\n                <div class=\"piece\" v-for=\"item in tasks\">\n                    <div class=\"piece_center\">\n                        <van-panel>\n                            <template #header>\n                                <div class=\"van-cell van-panel__header\">\n                                    <div class=\"van-cell__title\">\n                                         <span style=\"display: flex;align-items: center\">\n                            <el-tag :type=\"item.state == 0 ? 'danger':(item.state == 1 ? 'warning':'success')\"\n                                    style=\"margin-right: 5px\">{{item.state == 0 ? '待解决':(item.state == 1 ? '服务中':'已完成')}}</el-tag>\n                            {{item.taskTitle}}\n                        </span>\n                                        <div class=\"van-cell__label\">{{item.createTime | formatDate}}</div>\n                                    </div>\n                                    <div class=\"van-cell__value van-panel__header-value\">\n                                        <span style=\"font-size: 12px\" v-show=\"item.state == 0\" @click=\"cancel(item.id)\">取消任务</span>\n                                        <span style=\"font-size: 12px\" @click=\"showRelease(item.accept)\" v-show=\"item.state != 0\">查看接收人</span>\n                                    </div>\n                                </div>\n                            </template>\n                            <div style=\"padding: 10px 0px;font-size: 14px;\">\n                                <van-steps :active=\"item.state\">\n                                    <van-step>\n                                        发布成功\n                                    </van-step>\n                                    <van-step>服务中</van-step>\n                                    <van-step>交易完成</van-step>\n                                </van-steps>\n                                <p style=\"margin: 0 0 25px 0;\">\n                                    {{item.taskContext}}\n                                </p>\n                                <div class=\"task_icon\">\n                                    <i class=\"el-icon-money\" style=\"color: red;\"> {{item.reward}}元</i>\n                                    <el-button :type=\"item.state != 1? 'info' : 'primary'\" round size=\"mini\" :disabled =\"item.state != 1? true : false\" @click=\"complete(item.id)\">完成任务</el-button>\n\n                                </div>\n                            </div>\n                        </van-panel>\n                    </div>\n                </div>\n                <div style=\"text-align: center; margin-top: 10%\">\n                    <span v-if=\"tasks.length == 0\"><i class=\"el-icon-refresh-right\"></i>暂无发布任务</span>\n                </div>\n            </van-tab>\n            <van-tab title=\"我接收的\">\n                <div class=\"piece\" v-for=\"item in acceptTheTask\">\n                    <div class=\"piece_center\">\n                        <van-panel>\n                            <template #header>\n                                <div class=\"van-cell van-panel__header\">\n                                    <div class=\"van-cell__title\">\n                                         <span style=\"display: flex;align-items: center\">\n                            <el-tag :type=\"item.state == 0 ? 'danger':(item.state == 1 ? 'warning':'success')\"\n                                    style=\"margin-right: 5px\">{{item.state == 0 ? '待解决':(item.state == 1 ? '服务中':'已完成')}}</el-tag>\n                            {{item.taskTitle}}\n                        </span>\n                                        <div class=\"van-cell__label\">{{item.createTime | formatDate}}</div>\n                                    </div>\n                                    <div class=\"van-cell__value van-panel__header-value\">\n                                        <span style=\"font-size: 12px\" v-show=\"item.state == 0\" @click=\"cancel(item.id)\">取消任务</span>\n                                        <span style=\"font-size: 12px\" @click=\"showRelease(item.publish)\">查看发布人</span>\n                                    </div>\n                                </div>\n                            </template>\n                            <div style=\"padding: 10px 0px;font-size: 14px;\">\n                                <p style=\"margin: 0 0 25px 0;\">\n                                    {{item.taskContext}}\n                                </p>\n                                <div class=\"task_icon\">\n                                    <i class=\"el-icon-money\" style=\"color: red;\"> {{item.reward}}元</i>\n                                    <el-button :type=\"item.state != 1? 'info' : 'primary'\" round size=\"mini\" :disabled =\"item.state != 1? true : false\" @click=\"cancelTask(item.id)\">取消任务</el-button>\n                                </div>\n                            </div>\n                        </van-panel>\n                    </div>\n                </div>\n                <div style=\"text-align: center; margin-top: 10%\">\n                    <span v-if=\"acceptTheTask.length == 0\"><i class=\"el-icon-refresh-right\"></i>暂无接单</span>\n                </div>\n            </van-tab>\n        </van-tabs>\n\n        <van-action-sheet\n                v-model=\"show\"\n                title=\"个人信息\"\n                close-on-click-action\n        >\n            <van-collapse v-model=\"activeNames\" v-if=\"acceptId.length != []\">\n                <van-collapse-item title=\"姓名\" name=\"1\">{{acceptId.username}}</van-collapse-item>\n                <van-collapse-item title=\"电话\" name=\"2\">{{acceptId.phone}}</van-collapse-item>\n                <van-collapse-item title=\"学校\" name=\"3\">{{acceptId.school.name}}</van-collapse-item>\n                <van-collapse-item title=\"所在系\" name=\"4\">{{acceptId.dept.name}}</van-collapse-item>\n                <van-collapse-item title=\"所在班级\" name=\"5\">{{acceptId.aclass.name}}</van-collapse-item>\n            </van-collapse>\n        </van-action-sheet>\n    </div>\n</template>\n\n<script>\n    import {mapState,mapMutations} from 'vuex'\n    import {formatDate} from '@/util/date';\n    export default {\n        name: \"Task\",\n        data(){\n            return{\n                show:false,\n                activeNames: ['1','2','3','4','5'],\n                active:0,\n                //发布的任务\n                tasks:[],\n                //接收的任务\n                acceptTheTask:[],\n                //接收人信息\n                acceptId:[],\n            }\n        },\n        computed:{\n          ...mapState('user',['user'])\n        },\n        methods: {\n            ...mapMutations('user', ['setUser']),\n            onClick(name, title) {\n\n            },\n            //显示发布人或接收人\n            showRelease(val){\n                this.acceptId = val\n                this.show = !this.show\n            },\n            //取消任务\n            cancel(id){\n                this.$dialog.alert({\n                    title: '取消任务',\n                    message: '确定取消任务吗',\n                    showCancelButton:true\n                }).then(() => {\n                    this.$del(\"/task/\" + id)\n                    .then(res => {\n                        this.$get(\"/user\",{id:this.user.id})\n                        .then((rs) => {\n                            sessionStorage.setItem(\"user\", JSON.stringify(rs.data.user))\n                            this.setUser(JSON.parse(sessionStorage.getItem(\"user\")))\n                        })\n                        this.$msg(res.data.msg,\"success\" );\n                        this.newList()\n                    })\n                }).catch(()=>{\n\n                })\n            },\n            //获取我发布的任务\n            newList(){\n                this.$get(\"/task/published\",{id: this.user.id})\n                .then(res=>{\n                    this.tasks = res.data.task\n                })\n            },\n            //获取我接收的任务\n            newReceive() {\n                this.$get(\"/task/accepted\", {id: this.user.id})\n                .then(res => {\n                    this.acceptTheTask = res.data.task\n                    // console.log(this.acceptTheTask)\n                })\n            },\n            //完成任务\n            complete(id){\n                this.$dialog.alert({\n                    title: '提示',\n                    message: '确定接收人完成此任务了吗',\n                    showCancelButton:true\n                }).then(() => {\n                    this.$put('task/' + id)\n                    .then(res => {\n                        this.$msg(res.data.msg,\"success\" );\n                        this.newList()\n                    })\n                }).catch(()=>{\n\n                })\n            },\n            //取消接收的任务\n            cancelTask(id){\n                this.$dialog.alert({\n                    title: '提示',\n                    message: '确定取消该任务吗',\n                    showCancelButton:true\n                }).then(() => {\n                    this.$put('/task/takerCancel/' + id)\n                    .then(res => {\n                        this.$msg(res.data.msg,\"success\")\n                        this.newReceive()\n                    })\n                }).catch(()=>{\n\n                })\n            }\n        },\n        created() {\n            if(this.$route.params.index){\n                this.active = this.$route.params.index\n            }\n            this.newList()\n            this.newReceive()\n        },\n        filters: {\n            formatDate(time) {\n                let date = new Date(time);\n                return formatDate(date, 'yyyy-MM-dd hh:mm');\n            }\n        }\n    }\n</script>\n\n<style scoped lang=\"less\">\n    [class*=van-hairline]::after{\n        border: none;\n    }\n    .van-steps--horizontal{\n        padding: 0;\n    }\n    .van-cell::after{\n        left: 0px;\n        right:0px;\n        border-bottom: 1px solid #ccc;\n    }\n    .piece {\n        margin: 10px;\n        padding: 10px 10px 10px 10px;\n        border-radius: 10px;\n        background: white;\n        .van-cell{\n            padding: 5px 0px;\n        }\n    }\n\n    .task_icon{\n        display: flex;justify-content: space-between;align-items: center\n    }\n    /deep/ .van-step--horizontal .van-step__title{\n        font-size: 14px !important;\n    }\n</style>"],"sourceRoot":"src/views/Android/Task"}]}