AA,OAAA,EAAA;AAFA,SAAA;AAIA,OApBA;AAqBA,KAjFA;AAoFA;AACA,IAAA,MArFA,oBAqFA;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,KAAA;AACA,KAxFA;AA0FA;AACA,IAAA,KA3FA,mBA2FA;AACA,WAAA,IAAA,GAAA;AACA,QAAA,EAAA,EAAA,IADA;AAEA,QAAA,IAAA,EAAA,IAFA;AAGA,QAAA,GAAA,EAAA;AAHA,OAAA,CADA,CAMA;AACA,KAlGA;;AAoGA;AACA,IAAA,WArGA,yBAqGA;AACA,WAAA,WAAA,CAAA,OAAA,GAAA,CAAA;AACA,WAAA,OAAA;AACA,KAxGA;AAyGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AACA;AACA,IAAA,UA3HA,wBA2HA;AACA,WAAA,SAAA,CAAA,WAAA;AACA,WAAA,WAAA;AACA,KA9HA;AA+HA;AACA,IAAA,qBAhIA,iCAgIA,SAhIA,EAgIA;AACA,WAAA,GAAA,GAAA,SAAA,CAAA,GAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,EAAA;AAAA,OAAA,CAAA;AACA,WAAA,MAAA,GAAA,SAAA,CAAA,MAAA,KAAA,CAAA;AACA,WAAA,QAAA,GAAA,CAAA,SAAA,CAAA,MAAA;AACA;AApIA;AAvCA,CAAA","sourcesContent":["<template>\n  <div class=\"app-container\">\n    <!-- 搜索栏🔍 -->\n    <el-form :model=\"queryParams\" ref=\"queryForm\" size=\"small\" :inline=\"true\" v-show=\"showSearch\" label-width=\"68px\">\n      <el-form-item label=\"姓名\" prop=\"name\">\n        <el-input\n          v-model=\"queryParams.name\"\n          placeholder=\"请输入姓名\"\n          clearable\n          @keyup.enter.native=\"handleQuery\"\n        />\n      </el-form-item>\n      <el-form-item label=\"年龄\" prop=\"age\">\n        <el-input\n          v-model=\"queryParams.age\"\n          placeholder=\"请输入年龄\"\n          clearable\n          @keyup.enter.native=\"handleQuery\"\n        />\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" icon=\"el-icon-search\" size=\"mini\" @click=\"handleQuery\">搜索</el-button>\n        <el-button icon=\"el-icon-refresh\" size=\"mini\" @click=\"resetQuery\">重置</el-button>\n      </el-form-item>\n    </el-form>\n\n    <el-row :gutter=\"10\" class=\"mb8\">\n      <el-col :span=\"1.5\">\n        <el-button\n          type=\"primary\"\n          plain\n          icon=\"el-icon-plus\"\n          size=\"mini\"\n          @click=\"handleAdd\"\n        >新增</el-button>\n      </el-col>\n      <el-col :span=\"1.5\">\n        <el-button\n          type=\"success\"\n          plain\n          icon=\"el-icon-edit\"\n          size=\"mini\"\n          :disabled=\"single\"\n          @click=\"handleUpdate\"\n        >修改</el-button>\n      </el-col>\n      <el-col :span=\"1.5\">\n        <el-button\n          type=\"danger\"\n          plain\n          icon=\"el-icon-delete\"\n          size=\"mini\"\n          :disabled=\"multiple\"\n          @click=\"handleDelete\"\n        >删除</el-button>\n      </el-col>\n\n      <!-- <right-toolbar :showSearch.sync=\"showSearch\" @queryTable=\"getList\"></right-toolbar> -->\n    </el-row>\n\n    <el-table v-loading=\"loading\" :data=\"studs\" @selection-change=\"handleSelectionChange\">\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\" />\n      <el-table-column label=\"学生编号\" align=\"center\" prop=\"id\" />\n      <el-table-column label=\"姓名\" align=\"center\" prop=\"name\" />\n      <el-table-column label=\"年龄\" align=\"center\" prop=\"age\" />\n      <el-table-column label=\"操作\" align=\"center\" class-name=\"small-padding fixed-width\">\n        <template slot-scope=\"scope\">\n          <el-button\n            size=\"mini\"\n            type=\"text\"\n            icon=\"el-icon-edit\"\n            @click=\"handleUpdate(scope.row)\"\n          >修改</el-button>\n          <el-button\n            size=\"mini\"\n            type=\"text\"\n            icon=\"el-icon-delete\"\n            @click=\"handleDelete(scope.row)\"\n          >删除</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n    \n    <!-- 分页 -->\n    <!-- <pagination\n      v-show=\"total>0\"\n      :total=\"total\"\n      :page.sync=\"queryParams.pageNum\"\n      :limit.sync=\"queryParams.pageSize\"\n      @pagination=\"getList\"\n    /> -->\n\n    <!-- 添加或修改学生管理对话框 -->\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body>\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\n        <el-form-item label=\"姓名\" prop=\"name\">\n          <el-input v-model=\"form.name\" placeholder=\"请输入姓名\" />\n        </el-form-item>\n        <el-form-item label=\"年龄\" prop=\"age\">\n          <el-input v-model=\"form.age\" placeholder=\"请输入年龄\" />\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\n        <el-button @click=\"cancel\">取 消</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { listStuage, getStuage, delStuage, addStuage, updateStuage } from \"@s/api/student/stuage\";\n\nexport default {\n  name: \"Stuage\",\n  data() {\n    return {\n      // 遮罩层\n      loading: true,\n      // 选中数组\n      ids: [],\n      // 非单个禁用\n      single: true,\n      // 非多个禁用\n      multiple: true,\n      // 显示搜索条件\n      showSearch: true,\n      // 总条数\n      total: 0,\n      // 学生管理表格数据\n      stuageList: [],\n      // 弹出层标题\n      title: \"\",\n      // 是否显示弹出层\n      open: false,\n      // 查询参数\n      queryParams: {\n        pageNum: 1,\n        pageSize: 10,\n        name: null,\n        age: null\n      },\n      // 表单参数\n      form: {},\n      // 表单校验\n      rules: {},\n      studs: []\n    };\n  },\n  created() {\n    this.newList();\n  },\n  methods: {\n    /** 查询学生管理列表 */\n    newList() {\n      this.$get(\"/student/stuage/list\")\n      .then((rs) => {\n        this.studs = rs.data.rows\n        this.loading = false;\n        // console.log(this.studs)\n      })\n    },\n    /** 新增按钮操作 */\n    handleAdd() {\n      // this.reset();\n      this.open = true;\n      this.title = \"添加学生管理\";\n    },\n\n    /** 修改按钮操作 */\n    handleUpdate(row) {\n      // this.reset();\n      const id = row.id || this.ids\n      this.$get(\"/student/stuage/\"+id\n                    ).then(response => {\n        this.form = response.data.student;\n        this.open = true;\n        this.title = \"修改学生管理\";\n      });\n    },\n\n    /** 提交按钮 */\n    submitForm() {\n      this.$refs[\"form\"].validate(valid => {\n        if (valid) {\n          if (this.form.id != null) {\n              this.$put(\"/student/stuage\",this.form).then(response => {\n                this.$message(\"修改成功\");\n                this.reset();\n                this.open = false;\n                this.newList();\n              });\n\n          } else {\n             // console.log(this.form);\n              // this.$post(\"/student/stuage\",this.form,).then(response => {\n              //   this.$message(\"新增成功\");\n              //   this.open = false;\n              //   this.newList();\n              // });\n\n              addStuage(this.form).then(response => {\n              this.$modal.msgSuccess(\"新增成功\");\n              this.open = false;\n              this.newList();\n            });\n          }\n        }\n      });\n    },\n    /** 删除按钮操作 */\n    handleDelete(row) {\n      this.$confirm('此操作将永久删除该文件, 是否继续?', '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'warning',\n          \n        }).then(() => {\n          const ids = row.id || this.ids;\n          this.$del(\"/student/stuage/\"+ids).then(response => {\n            this.open = false;\n            this.newList();\n          });\n          this.$message({\n            type: 'success',\n            message: '删除成功!'\n            });\n        }).catch(() => {\n          this.$message({\n            type: 'info',\n            message: '已取消删除'\n          });          \n        });\n    },\n\n\n    // 取消按钮\n    cancel() {\n      this.open = false;\n      this.reset();\n    },\n\n    // 表单重置\n    reset() {\n      this.form = {\n        id: null,\n        name: null,\n        age: null\n      };\n      // this.resetForm(\"form\");\n    },\n\n    /** 搜索按钮操作 */\n    handleQuery() {\n      this.queryParams.pageNum = 1;\n      this.newList();\n    },\n    // clickSearch() {\n    //             let parameter = {};\n    //             if (this.input) {\n    //                 if (this.select == 1) {\n    //                     parameter = {\"name\": this.input};\n    //                 } else {\n    //                     parameter = {\"age\": this.input};\n    //                 }\n    //                 this.$get(\"/student/stuage\", parameter)\n    //                 .then((res) => {\n    //                     this.users = res.data.user\n    //                     this.input = \"\"\n    //                 })\n    //             } else {\n    //                 this.newList()\n    //             }\n    // },\n    /** 重置按钮操作 */\n    resetQuery() {\n      this.resetForm(\"queryForm\");\n      this.handleQuery();\n    },\n    // 多选框选中数据\n    handleSelectionChange(selection) {\n      this.ids = selection.map(item => item.id)\n      this.single = selection.length!==1\n      this.multiple = !selection.length\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/views/student"}]}