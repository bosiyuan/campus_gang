:gutter=\"10\" class=\"mb8\">\n      <el-col :span=\"1.5\">\n        <el-button\n          type=\"primary\"\n          plain\n          icon=\"el-icon-plus\"\n          size=\"mini\"\n          @click=\"handleAdd\"\n        >Êñ∞Â¢û</el-button>\n      </el-col>\n      <el-col :span=\"1.5\">\n        <el-button\n          type=\"success\"\n          plain\n          icon=\"el-icon-edit\"\n          size=\"mini\"\n          :disabled=\"single\"\n          @click=\"handleUpdate\"\n        >‰øÆÊîπ</el-button>\n      </el-col>\n      <el-col :span=\"1.5\">\n        <el-button\n          type=\"danger\"\n          plain\n          icon=\"el-icon-delete\"\n          size=\"mini\"\n          :disabled=\"multiple\"\n          @click=\"handleDelete\"\n        >Âà†Èô§</el-button>\n      </el-col>\n\n      <!-- <right-toolbar :showSearch.sync=\"showSearch\" @queryTable=\"getList\"></right-toolbar> -->\n    </el-row>\n\n    <el-table v-loading=\"loading\" :data=\"studs\" @selection-change=\"handleSelectionChange\">\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\" />\n      <el-table-column label=\"Â≠¶ÁîüÁºñÂè∑\" align=\"center\" prop=\"id\" />\n      <el-table-column label=\"ÂßìÂêç\" align=\"center\" prop=\"name\" />\n      <el-table-column label=\"Âπ¥ÈæÑ\" align=\"center\" prop=\"age\" />\n      <el-table-column label=\"Êìç‰Ωú\" align=\"center\" class-name=\"small-padding fixed-width\">\n        <template slot-scope=\"scope\">\n          <el-button\n            size=\"mini\"\n            type=\"text\"\n            icon=\"el-icon-edit\"\n            @click=\"handleUpdate(scope.row)\"\n          >‰øÆÊîπ</el-button>\n          <el-button\n            size=\"mini\"\n            type=\"text\"\n            icon=\"el-icon-delete\"\n            @click=\"handleDelete(scope.row)\"\n          >Âà†Èô§</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n    \n    <!-- ÂàÜÈ°µ -->\n    <!-- <pagination\n      v-show=\"total>0\"\n      :total=\"total\"\n      :page.sync=\"queryParams.pageNum\"\n      :limit.sync=\"queryParams.pageSize\"\n      @pagination=\"getList\"\n    /> -->\n\n    <!-- Ê∑ªÂä†Êàñ‰øÆÊîπÂ≠¶ÁîüÁÆ°ÁêÜÂØπËØùÊ°Ü -->\n    <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"500px\" append-to-body>\n      <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"80px\">\n        <el-form-item label=\"ÂßìÂêç\" prop=\"name\">\n          <el-input v-model=\"form.name\" placeholder=\"ËØ∑ËæìÂÖ•ÂßìÂêç\" />\n        </el-form-item>\n        <el-form-item label=\"Âπ¥ÈæÑ\" prop=\"age\">\n          <el-input v-model=\"form.age\" placeholder=\"ËØ∑ËæìÂÖ•Âπ¥ÈæÑ\" />\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button type=\"primary\" @click=\"submitForm\">Á°Æ ÂÆö</el-button>\n        <el-button @click=\"cancel\">Âèñ Ê∂à</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { listStuage, getStuage, delStuage, addStuage, updateStuage } from \"@s/api/student/stuage\";\n\nexport default {\n  name: \"Stuage\",\n  data() {\n    return {\n      // ÈÅÆÁΩ©Â±Ç\n      loading: true,\n      // ÈÄâ‰∏≠Êï∞ÁªÑ\n      ids: [],\n      // ÈùûÂçï‰∏™Á¶ÅÁî®\n      single: true,\n      // ÈùûÂ§ö‰∏™Á¶ÅÁî®\n      multiple: true,\n      // ÊòæÁ§∫ÊêúÁ¥¢Êù°‰ª∂\n      showSearch: true,\n      // ÊÄªÊù°Êï∞\n      total: 0,\n      // Â≠¶ÁîüÁÆ°ÁêÜË°®Ê†ºÊï∞ÊçÆ\n      stuageList: [],\n      // ÂºπÂá∫Â±ÇÊ†áÈ¢ò\n      title: \"\",\n      // ÊòØÂê¶ÊòæÁ§∫ÂºπÂá∫Â±Ç\n      open: false,\n      // Êü•ËØ¢ÂèÇÊï∞\n      queryParams: {\n        pageNum: 1,\n        pageSize: 10,\n        name: null,\n        age: null\n      },\n      // Ë°®ÂçïÂèÇÊï∞\n      form: {},\n      // Ë°®ÂçïÊ†°È™å\n      rules: {},\n      studs: []\n    };\n  },\n  created() {\n    this.newList();\n  },\n  methods: {\n    /** Êü•ËØ¢Â≠¶ÁîüÁÆ°ÁêÜÂàóË°® */\n    newList() {\n      this.$get(\"/student/stuage/list\")\n      .then((rs) => {\n        this.studs = rs.data.rows\n        this.loading = false;\n        // console.log(this.studs)\n      })\n    },\n    /** Êñ∞Â¢ûÊåâÈíÆÊìç‰Ωú */\n    handleAdd() {\n      // this.reset();\n      this.open = true;\n      this.title = \"Ê∑ªÂä†Â≠¶ÁîüÁÆ°ÁêÜ\";\n    },\n\n    /** ‰øÆÊîπÊåâÈíÆÊìç‰Ωú */\n    handleUpdate(row) {\n      // this.reset();\n      const id = row.id || this.ids\n      this.$get(\"/student/stuage/\"+id\n                    ).then(response => {\n        this.form = response.data.student;\n        this.open = true;\n        this.title = \"‰øÆÊîπÂ≠¶ÁîüÁÆ°ÁêÜ\";\n      });\n    },\n\n    /** Êèê‰∫§ÊåâÈíÆ */\n    submitForm() {\n      this.$refs[\"form\"].validate(valid => {\n        if (valid) {\n          if (this.form.id != null) {\n              this.$put(\"/student/stuage\",this.form).then(response => {\n                this.$message(\"‰øÆÊîπÊàêÂäü\");\n                this.reset();\n                this.open = false;\n                this.newList();\n              });\n\n          } else {\n             // console.log(this.form);\n              // this.$post(\"/student/stuage\",this.form,).then(response => {\n              //   this.$message(\"Êñ∞Â¢ûÊàêÂäü\");\n              //   this.open = false;\n              //   this.newList();\n              // });\n\n              addStuage(this.form).then(response => {\n              this.$modal.msgSuccess(\"Êñ∞Â¢ûÊàêÂäü\");\n              this.open = false;\n              this.newList();\n            });\n          }\n        }\n      });\n    },\n    /** Âà†Èô§ÊåâÈíÆÊìç‰Ωú */\n    handleDelete(row) {\n      this.$confirm('Ê≠§Êìç‰ΩúÂ∞ÜÊ∞∏‰πÖÂà†Èô§ËØ•Êñá‰ª∂, ÊòØÂê¶ÁªßÁª≠?', 'ÊèêÁ§∫', {\n          confirmButtonText: 'Á°ÆÂÆö',\n          cancelButtonText: 'ÂèñÊ∂à',\n          type: 'warning',\n          \n        }).then(() => {\n          const ids = row.id || this.ids;\n          this.$del(\"/student/stuage/\"+ids).then(response => {\n            this.open = false;\n            this.newList();\n          });\n          this.$message({\n            type: 'success',\n            message: 'Âà†Èô§ÊàêÂäü!'\n            });\n        }).catch(() => {\n          this.$message({\n            type: 'info',\n            message: 'Â∑≤ÂèñÊ∂àÂà†Èô§'\n          });          \n        });\n    },\n\n\n    // ÂèñÊ∂àÊåâÈíÆ\n    cancel() {\n      this.open = false;\n      this.reset();\n    },\n\n    // Ë°®ÂçïÈáçÁΩÆ\n    reset() {\n      this.form = {\n        id: null,\n        name: null,\n        age: null\n      };\n      // this.resetForm(\"form\");\n    },\n\n    /** ÊêúÁ¥¢ÊåâÈíÆÊìç‰Ωú */\n    handleQuery() {\n      this.queryParams.pageNum = 1;\n      this.newList();\n    },\n    // clickSearch() {\n    //             let parameter = {};\n    //             if (this.input) {\n    //                 if (this.select == 1) {\n    //                     parameter = {\"name\": this.input};\n    //                 } else {\n    //                     parameter = {\"age\": this.input};\n    //                 }\n    //                 this.$get(\"/student/stuage\", parameter)\n    //                 .then((res) => {\n    //                     this.users = res.data.user\n    //                     this.input = \"\"\n    //                 })\n    //             } else {\n    //                 this.newList()\n    //             }\n    // },\n    /** ÈáçÁΩÆÊåâÈíÆÊìç‰Ωú */\n    resetQuery() {\n      this.resetForm(\"queryForm\");\n      this.handleQuery();\n    },\n    // Â§öÈÄâÊ°ÜÈÄâ‰∏≠Êï∞ÊçÆ\n    handleSelectionChange(selection) {\n      this.ids = selection.map(item => item.id)\n      this.single = selection.length!==1\n      this.multiple = !selection.length\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/views/student"}]}