A,YAAA,CAAA,CAAA,WAAA,CAAA,CADA;AAEA,IAAA,QAFA,oBAEA,EAFA,EAEA;AACA,WAAA,EAAA,GAAA,EAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,KALA;AAMA,IAAA,IANA,gBAMA,EANA,EAMA;AAAA;;AACA,WAAA,OAAA,CAAA,OAAA,CAAA;AACA,QAAA,KAAA,EAAA,IADA;AAEA,QAAA,OAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,kBAAA,EAAA;AAAA,UAAA,EAAA,EAAA,EAAA;AAAA,sBAAA,KAAA,CAAA,IAAA,CAAA;AAAA,SAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA,CAAA,GAAA,EAAA,SAAA;;AACA,UAAA,KAAA,CAAA,SAAA,CAAA,CAAA;;AACA,UAAA,cAAA,CAAA,OAAA,CAAA,QAAA,EAAA,CAAA;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA,OAAA;AAAA,YAAA,MAAA,EAAA;AAAA,cAAA,KAAA,EAAA;AAAA;AAAA,WAAA;AACA,SANA;AAOA,OAZA,EAaA,KAbA,CAaA,YAAA,CACA;AACA,OAfA;AAgBA,KAvBA;AAwBA,IAAA,IAxBA,kBAwBA;AAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KA1BA;AA2BA,IAAA,QA3BA,sBA2BA;AACA,WAAA,IAAA,CAAA,aAAA,EAAA,OAAA;AACA;AA7BA,IAjBA;AAiDA,EAAA,OAjDA,qBAiDA;AAAA;;AACA,SAAA,IAAA,CAAA,OAAA,EAAA;AAAA,MAAA,EAAA,EAAA,KAAA,IAAA,CAAA;AAAA,KAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,KAHA,EADA,CAKA;AACA,GAvDA;AAwDA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,IADA,EACA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,CAAA,IAAA,CAAA;AACA,aAAA,WAAA,CAAA,IAAA,EAAA,kBAAA,CAAA;AACA;AAJA;AAxDA,CAAA","sourcesContent":["<template>\n    <div class=\"home\">\n        <van-search v-model=\"value\" placeholder=\"请输入搜索关键词\" show-action>\n            <template #action>\n                <div @click=\"onSearch\">搜索</div>\n            </template>\n        </van-search>\n        <van-swipe class=\"swipe\" :autoplay=\"3000\">\n            <van-swipe-item v-for=\"(image, index) in images\" :key=\"index\">\n                <img :src=\"image\"/>\n            </van-swipe-item>\n        </van-swipe>\n        <van-notice-bar style=\"margin: 5px 0px\"\n                        scrollable\n                        left-icon=\"volume-o\"\n                        text=\"校园帮APP上线了。\"\n                        background=\"#8a97f6\"\n                        color=\"#fff\"\n        />\n        <div class=\"center\">\n            <div class=\"piece\" v-for=\"item in tasks\" v-if=\"item.state == 0\">\n                <div class=\"piece_center\">\n                    <van-panel>\n                        <template #header>\n                            <div class=\"van-cell van-panel__header\">\n                                <div class=\"van-cell__title\">\n                                    <span><van-tag type=\"success\"\n                                                   style=\"padding: 2px 5px;margin-right: 10px\">待解决</van-tag>{{item.taskTitle}}</span>\n                                    <div class=\"van-cell__label\">{{item.createTime | formatDate}}</div>\n                                </div>\n                                <div class=\"van-cell__value van-panel__header-value\">\n                                    <span style=\"font-size: 12px\" v-show=\"item.publish.id == user.id\">本人任务</span>\n                                    <span style=\"font-size: 12px\" @click=\"jump(item.id)\"\n                                          v-show=\"item.publish.id != user.id\">接受任务</span>\n                                </div>\n                            </div>\n                        </template>\n                        <div style=\"padding: 10px 0px;font-size: 14px;\">\n                            <p style=\"margin-bottom: 5px;\">\n                                {{item.taskContext}}\n                            </p>\n                            <van-tag type=\"danger\" style=\"padding: 2px;float: right;font-size: 12px\">悬赏\n                                {{item.reward}}元\n                            </van-tag>\n                        </div>\n                    </van-panel>\n                </div>\n            </div>\n        </div>\n        <van-button @click=\"plus\" icon=\"plus\" type=\"info\"\n                    style=\"border-radius: 100px;width: 40px;height: 40px;position: fixed;bottom: 60px;right: 20px;\"/>\n        <router-view></router-view>\n    </div>\n</template>\n\n<script>\n    import {mapMutations, mapState} from 'vuex';\n    import {formatDate} from '@/util/date';\n\n    export default {\n        name: 'Home',\n        data() {\n            return {\n                value: '',\n                show: false,\n                images: [\n                    require('../../../assets/images/pic01.png'),\n                    require('../../../assets/images/pic02.png'),\n                    require('../../../assets/images/pic03.png'),\n                ],\n                tasks: []\n            }\n        },\n        computed: {\n            ...mapState('user', ['user'])\n        },\n        methods: {\n            ...mapMutations(['setActive']),\n            messages(id) {\n                this.id = id;\n                this.show = true\n            },\n            jump(id) {\n                this.$dialog.confirm({\n                    title: '提示',\n                    message: '是否接受该任务',\n                })\n                    .then(() => {\n                        this.$put('task/takerAccept', {id: id, \"acceptId\": this.user.id})\n                            .then(res => {\n                                this.$msg(res.data.msg, \"success\");\n                                this.setActive(1)\n                                sessionStorage.setItem(\"active\", 1)\n                                this.$router.push({name: 'mTask', params: {index: 1}})\n                            })\n                    })\n                    .catch(() => {\n                        // on cancel\n                    });\n            },\n            plus() {//跳转到发布页面\n                this.$router.push({name: 'mrelease'})\n            },\n            onSearch() {\n                this.$msg(\"敬请期待,正在努力开发\", \"error\")\n            }\n\n        },\n        created() {\n            this.$get(\"/task\", {id: this.user.id})\n                .then((res) => {\n                    this.tasks = res.data.task\n                })\n            // console.log(this.user)\n        },\n        filters: {\n            formatDate(time) {\n                let date = new Date(time);\n                return formatDate(date, 'yyyy-MM-dd hh:mm');\n            }\n        }\n    }\n</script>\n\n<style lang=\"less\">\n    .swipe img {\n        height: 33vh;\n        width: 100%;\n    }\n\n    [class*=van-hairline]::after {\n        border: none;\n    }\n\n    .van-steps--horizontal {\n        padding: 0;\n    }\n\n    .van-cell::after {\n        left: 0px;\n        right: 0px;\n        border-bottom: 1px solid #ccc;\n    }\n\n    .piece {\n        margin: 10px;\n        padding: 10px 10px 20px 10px;\n        border-radius: 10px;\n        background: white;\n\n        .van-cell {\n            padding: 5px 0px;\n        }\n    }\n\n    .van-search__action {\n        padding: 0 12px 0 8px !important;\n    }\n\n</style>\n"],"sourceRoot":"src/views/Android/Home"}]}