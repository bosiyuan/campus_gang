sContent":["<template>\n    <div class=\"content\">\n\n        <el-row>\n        <el-col :span=\"24\"><div class=\"grid-content bg-purple-dark\">欢迎登录---校园跑腿管理系统</div></el-col>\n        </el-row>\n\n        <el-alert\n                :title=\"user.school.name + '- - -任务平台共' + tasks.length + '个任务'\"\n                :closable=\"false\"\n                type=\"info\">\n        </el-alert>\n\n        <div class=\"title\">\n            <div class=\"title_center\">\n                <h2>{{tasks.length}}</h2>\n                <p>任务数量</p>\n            </div>\n            <div class=\"title_right\">\n                <h2>{{users.length}}</h2>\n                <p>总用户</p>\n            </div>\n        </div>\n\n    </div>\n</template>\n\n<script>\n    import {mapMutations, mapState} from \"vuex\";\n\n    export default {\n        name: \"Index\",\n        data() {\n            return {\n                tasks: [],\n                users: [],\n            }\n        },\n\n        methods: {\n        },\n\n        computed: {\n            ...mapState('user', ['user'])\n        },\n        created() {\n            // console.log(this.user)\n            this.$get(\"/task\", {id: this.user.id})\n                .then((res) => {\n                    this.tasks = res.data.task\n            })\n\n            this.$get(\"/user\")\n            .then((rs) => {\n                this.users = rs.data.user\n            })\n\n        },\n\n    }\n</script>\n\n<style scoped lang=\"less\">\n\n    .el-row {\n        margin-bottom: 20px;\n        &:last-child {\n        margin-bottom: 0;\n        }\n    }\n    .el-col {\n        border-radius: 4px;\n    }\n\n      .bg-purple {\n    background: #d3dce6;\n  }\n\n    .content {\n        background: #FFf;\n        margin: 0 15px;\n        padding: 15px;\n\n        .card h2 {\n            font-weight: normal;\n            font-size: 18px;\n\n            span {\n                font-size: 12px;\n                display: inline-block;\n                border: 1px solid red;\n                padding: 1px 3px;\n            }\n        }\n\n        .title {\n            margin: 15px 0;\n            width: 100%;\n            display: flex;\n            height: 100px;\n            justify-content: space-between;\n\n            div {\n                border-radius: 5px;\n                padding: 15px;\n                color: #fff;\n            }\n\n            div p {\n                font-size: 12px;\n                margin-top: 10px;\n            }\n\n            .title_center {\n                width: 50%;\n                background: #d8b34d;\n            }\n\n            .title_right {\n                width: 50%;\n                background: #1f68bb;\n            }\n        }\n\n    }\n\n</style>"],"sourceRoot":"src/views/PC/user/children"}]}